{
	"name": "infra",
	"version": "1.0.0",
	"dependencies": {
		"@cdktf/provider-aws": "^19.14.0",
		"@cdktf/provider-google": "^13.13.0",
		"cdktf": "^0.20.6",
		"constructs": "^10.3.0"
	},
	"devDependencies": {
		"@econominhas/eslint-config": "^1.0.0",
		"@types/node": "20.11.30",
		"env-cmd": "^10.1.0",
		"eslint": "^8.57.0",
		"prettier": "^3.2.5",
		"typescript": "~5.4.3"
	},
	"scripts": {
		"deploy": "env-cmd cdktf deploy --app=\"pnpm run build && node index.js\"",
		"destroy": "env-cmd cdktf destroy --app=\"pnpm run build && node index.js\"",
		"build": "tsc",
		"synth": "cdktf synth",
		"upgrade": "pnpm i cdktf@latest cdktf-cli@latest",
		"upgrade:next": "pnpm i cdktf@next cdktf-cli@next"
	}
}
